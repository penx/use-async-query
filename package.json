{
  "name": "use-async-query",
  "version": "1.0.0",
  "description": "Apollo Client's useQuery API for any async function",
  "module": "index.js",
  "main": "cjs/index.js",
  "scripts": {
    "test": "jest --coverage --verbose && tsd && npm run lint",
    "jest": "jest --verbose --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run build:ecm && npm run build:commonjs",
    "build:ecm": "tsc -p tsconfig.prod.json",
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "lint": "eslint '**/*.ts'"
  },
  "author": "Alasdair McLeay",
  "license": "ISC",
  "keywords": [
    "apollo",
    "apollo-client",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penx/use-async-query.git"
  },
  "bugs": {
    "url": "https://github.com/penx/use-async-query/issues"
  },
  "homepage": "https://github.com/penx/use-async-query#readme",
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.4.1",
    "eslint": "^8.13.0",
    "eslint-config-react-app": "^7.0.0",
    "jest": "^27.5.1",
    "react": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^27.1.3",
    "tsd": "^0.19.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@wry/equality": "^0.5.2"
  }
}
